<div class="text-center">
  <h1>{{data.displayName}}</h1>
</div>
<div class="container text-center">
  <div class="row">
      <div class="col-12 col-md-4 product mb-3">
        <div class="card h-100">
          <img src={{data.displayIcon}} class="card-img-top" alt="{{data.displayName}}" />
          <div class="card-body">
            <p class="card-text">
              {{data.description}}
            </p>
          </div>
        </div>
      </div>
  </div>
</div>