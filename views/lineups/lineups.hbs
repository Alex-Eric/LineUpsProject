<div class="text-center">
  <h1>LINEUPS</h1>
</div>
<div class="text-end m-5">
  <a href="/lineups/create" class="btn btn-primary">Create lineup!</a>
</div>
<div class="container text-center">
  <div class="row">
    {{#each lineup}}
      <div class="col-12 col-md-4 product mb-3">
        <div class="card h-100">
            <h2>{{title}}</h2>
          <video poster="placeholder.png">
            <source src={{videoLineup}} type="video/mp4" />
            Your browser does not support HTML video.
          </video>
          <div class="card-body">
            <h5 class="card-title">{{agent}}</h5>
            <h5 class="card-title">{{map}}</h5>
            <h5 class="card-title">{{attackDefense}}</h5>
            <a href="/lineups/{{_id}}" class="btn btn-primary">More details</a>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>